section.section-main
    .wrapper-container.d-flex 
        .wrapper-left 
            include ./sidebar.pug
        .wrapper-right 
            .wrapper-right_header.d-flex.align-items-center.justify-content-between 
                h2.wrapper-right_header--left Tạo form yêu cầu
                .wrapper-right_header--right.d-flex.align-items-center
                    .wrapper-right_header--right__select
                        select#year
                            option(value='hide') Năm
                            option(value='2022') 2022
                            option(value='2021') 2021
                            option(value='2020') 2020
                            option(value='2019') 2019
                            option(value='2018') 2018
                            option(value='2017') 2017
                            option(value='2016') 2016
                            option(value='2015') 2015
                            option(value='2014') 2014
                            option(value='2013') 2013
                            option(value='2012') 2012
                            option(value='2011') 2011
                            option(value='2010') 2010
                        select#mounth
                            option(value='hide') Tháng
                            option(value='january') Tháng 1
                            option(value='february') Tháng 2
                            option(value='march') Tháng 3
                            option(value='april') Tháng 4
                            option(value='may') Tháng 5
                            option(value='june') Tháng 6
                            option(value='july') Tháng 7
                            option(value='august') Tháng 8
                            option(value='september') Tháng 9
                            option(value='october') Tháng 10
                            option(value='november') Tháng 11
                            option(value='december') Tháng 12
                    .wrapper-right_header--right__sort 
                        a.active(href="") Tháng 
                        a(href="") Năm  
            .wrapper-right_body
                .wrapper-right_body--grid.d-flex.align-items-center
                    .grid-item 
                        .grid-item_content 
                            .grid-item_content--icon 
                                img(src="./images/icons/news.svg", alt="")
                            .grid-item_content--title 
                                h3 Mở hợp đồng bảo hiểm
                            .attach 
                                img(src="./images/icons/attach.svg", alt="")
                    .grid-item 
                        .grid-item_content 
